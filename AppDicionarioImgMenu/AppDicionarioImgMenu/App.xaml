<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:AppDicionarioImgMenu"
             x:Class="AppDicionarioImgMenu.App">
    <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="corTitulo">#121B32</Color>
            <Color x:Key="corTituloClaro">purple</Color>
            <Color x:Key="corTexto">blue</Color>
            <Color x:Key="corSaibaMais">#E1E7EB</Color>
            <x:String x:Key="imgWorld">icon_world.png</x:String>
            <x:Double x:Key="tamanhoFontTitulo">25</x:Double>
            <x:Double x:Key="tamanhoFontTexto">20</x:Double>

            <!--CÓDIGO ABAIXO JA VEM COM O MAUI, NÃO PRECISA COPIAR-->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
