<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppDicionarioImgMenu.pgMenu"
             xmlns:local=
                "clr-namespace:AppDicionarioImgMenu"
             Title="pgMenu"
             Shell.TabBarIsVisible="False"
             Shell.NavBarIsVisible="False">
    <FlyoutItem Route="Principal"
                FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="MainPage"
                      ContentTemplate=
                      "{DataTemplate local:MainPage}"/>
        <ShellContent Title="Principal"
                      ContentTemplate=
                      "{DataTemplate local:pgPrincipal}"/>
        
    </FlyoutItem>
</Shell>